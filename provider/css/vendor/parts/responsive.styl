@media (max-width: 1500px)

  .slideshow-main
    .slick-prev
      left 9%
    .slick-next
      right 9%




@media (max-width: 1300px)
  .wrapper
    width 94%
  .wrapper-fluid 
    width 100%




@media (max-width: 1199px)  

  .navigation-small-2
    display block


  .footer-nav-list-2
    margin-top 50px

  .slideshow-main
    .slick-prev
      left 5%
    .slick-next
      right 5%



  .parent-nav
    position absolute
    left 0
    top 100%    
    width 360px
    background #293255
    display none
  
  .nav
    width 100%
    ul
      width 100%
      flex-direction column
    li 
      border-bottom #aaafc0 1px solid
      &:hover
        &:after
          display none
    a
      min-height 50px
      padding 0 25px
      justify-content flex-start
      width auto
      span
        width 100%
      &:after
        display none

  .sub-nav
    position relative
    width 100%
    padding 0px
    opacity 1	
    a 
      padding 0 47px!important


  .parent-nav
    .nav-small
      width 100%
      display flex
      flex-direction column-reverse
      align-items flex-start
      margin-top 25px
    .btn-reserve
      width 100%
      border-radius 0
      margin-top 80px
    .btn-fb
      margin-left 40px



  .button-nav-mobile
    position absolute
    width 90px
    height 100%
    background #293255
    left 0
    top 0
    .icons
      position absolute
      top 50%
      left 50%
      transform translate(-50%, -50%)
  
  .parent-logo
    margin-left 90px

  .button-nav-mobile
    display block


  .nav-show
    .parent-nav
      display block
    .icon-button-nav-mobile
      background-image url(../../img/icons/nav-close.png?v=1)

  .nav li
    flex-direction column
  .nav a
    width 100%
  .nav li.active:after
    bottom -1px

  
  .nav a.with-subnav span
    padding 7px 22px 7px 22px

  
  .nav li.active:after
    display none !important
  



@media (max-width: 1023px)
  .do-vertical-line:before
    display none
  .do-vertical-line > div
    padding-left 0!important
    padding-right 0!important
  .parent-description
    margin-top 40px
    padding 0 20px
  .group-form .g-form-col
    flex-direction column
  .g-form-col .box-form
    width 100%
    margin 0 0 30px 0    

  .detail-style-1
    .flex
      flex-direction column-reverse
      > div
        width 100%!important
  
  .detail-style-1 
    .flex .col-1
      margin-top 60px

    .header-parent-title
      flex-direction column
      align-items flex-start
      .button-detail
        margin-top 20px
    .detail-body
      margin-top 25px
    

  .body.servicios .list-type-1
    margin-top 0
    padding 45px 0
    background #fff



  .block-form
    .g-form-col-2-type-1   
      flex-direction row       

  



@media (max-width: 767px)

  .space-header
	  height 141px

  .hide-m
    display none!important
  .show-m
    display block!important


  .footer-nav-list-2
    flex-direction column
  .do-line
    border 0
    padding-left .5rem
    padding-right .5rem
  .footer-nav-list-2
    margin-top 0
    > div
      min-height 38px
      text-align left
      border-bottom 1px solid #323a5c
      display flex
      align-items center
      .title
        padding-left .5rem
  .footer-row .title:after
    display none
  .footer-nav-2
    border-top 0
    margin-top 0
  .footer-nav-3, .footer-nav-3-child
    display flex
    flex-direction column  
  .brand-2
    margin-top 60px    
  .footer
    padding-bottom 40px

  

  .slick-arrow
    width 33px
    height 33px

  .legals
    display flex
    margin-top 20px
    flex-direction column
    a  
      margin 8px 0!important


  .slideshow-main
    .slick-prev
      left 3%
    .slick-next
      right 3%


  .header-top-list 
    padding-top 10px
    padding-bottom 10px
    height auto
    a
      font-size 12px
      margin 2px 0 2px 0!important
    .icons
      background-size 90%


  .parent-nav
    position fixed
    top 140px
    left 0
    height calc(100% - 140px)
    overflow auto

  
  .box-form-check
    flex-direction column
  .box-form-check .i-check-2
    margin 15px 0 0 0
  .group-form .g-form-col, .block-form > .box-form
    margin-top 15px
  .form-item
    height 45px


  .body.contacto
    .caption-type-1
      padding-left 10px !important
      padding-right 10px !important

  .list-nav-type-1 .list-nav
    display none
  .list-nav.mobile
    display block
    select
      height 60px
      width 100%




@media (max-width: 630px)

  .parent-nav    
    top 125px    
    height calc(100% - 124px)

  .space-header
	  height 125px

  .navigation-small-2
    display none

  .container-fluid
    padding-left 20px
    padding-right 20px

  .caption-type-1 .parent-title-2 *, .list-type-1 .parent-title *, .services-list .parent-title * strong
    font-size 24px
  .caption-type-1 .description
    font-size 13px
  .slideshow-main .img
    padding-top 416px

  .slideshow-confidence .title-1
    font-size 24px

  .appointment
    background-image url("../../img/appointment-m.jpg")!important
    .caption
      padding-bottom 12rem

  .accounts 
    .item-2
      background-image url("../../img/accounts-img-m.jpg")!important
  
  .list-type-1
    padding-top 70px

  .accordion-button i
    right 0
    position absolute

  .snt-2    
    background-image url(../../img/notario-people-m.jpg?v=1)
    .caption
      padding-bottom 84%

    
  .our-values
    margin-top 110px

  .breadcrumb .icon-triangle-right-2
    left 20px
    top 4px


  .detail-style-1 .header-parent-title h2
    font-size 24px




@media (max-width: 575px)
  .container-fluid
    padding-left 20px
    padding-right 20px
  .list-type-1 .box
    padding 0 40px
  .slideshow-type-1 .slick-prev
    left -45px
  .slideshow-type-1 .slick-next
    right -45px
  .caption-type-1
    padding-left 40px
    padding-right 40px
  .breadcrumb a
    font-size 13px
  .box-cat, .section.tyc
    margin-top 40px
  .section.tyc .item-body
    margin-top 30px
  .section.tyc .caption-type-1
    padding 0 10px  

  .tramites
    .caption-type-1
      padding 0 10px




@media (max-width: 520px)
  .parent-nav    
    top 140px       
    height calc(100% - 140px)

  .space-header
	  height 140px
  .caption-type-1
    padding-left 30px
    padding-right 30px
  .slideshow-type-1 .slick-prev
    left -50px
  .slideshow-type-1 .slick-next
    right -50px
  .list-type-1 .box
    padding 0 35px
  .slideshow-main .img
    padding-top 448px
  .services-list 
    .parent-title * 
      font-size 18px
      strong    
        font-size 22px
  .logo
    width 175px

  .box-form-check label:before
    width 20px
    height 20px
  .parent-iframe-map .iframe-map
    height 400px


  .block-form
    .g-form-col-2-type-1   
      flex-direction column
      .box-form
        width 100%!important
        &:last-child 
          margin-bottom 0
  



@media (max-width: 400px)

  .parent-nav
    width 100%
    margin 0
  .nav a
    padding 0 25px 0 17px
  
  .list-accounts
    padding-left 0
    padding-right 0
  
  .sub-nav
    a 
      padding 0 42px!important


@media (max-width: 320px)
  .space-header
	  height 160px

